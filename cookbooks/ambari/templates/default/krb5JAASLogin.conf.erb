com.sun.security.jgss.krb5.initiate {
    com.sun.security.auth.module.Krb5LoginModule required
    renewTGT=false
    doNotPrompt=true
    useKeyTab=true
    keyTab="<%= node['ambari']['kerberos']['keytab']['location']%>"
    principal="<%= @ambari_principal %>"
    storeKey=true
    useTicketCache=false;
};
