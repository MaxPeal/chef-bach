
[default]
debug_level = INFO
hostname = <%=node['ams']['hostname']%>
metrics_servers = <%=node['ams']['metrics_collector']['hosts']%>
enable_time_threshold = false
enable_value_threshold = false
skip_disk_patterns = True
instanceId=<%=node['ams']['clustername']%>
set.instanceId=false

[emitter]
send_interval = 60

[collector]
collector_sleep_interval = 10
max_queue_size = 5000
failover_strategy = round-robin
failover_strategy_blacklisted_interval_seconds = 300
port = 6188
https_enabled = False
