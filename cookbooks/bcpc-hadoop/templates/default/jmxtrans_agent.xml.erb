<jmxtrans-agent>
  <queries>
    <% (@queries || {}).each do |query| %>
      <query objectName="<%= query['objectName'] %>" attributes="<%= query['attributes'] %>" resultAlias="<%= query['resultAlias'] %>" type="<%= query['type'] %>"/>
    <% end %>
  </queries>

  <% @output_writers.each do |writer_name|  %>
    <% writer = eval "@#{writer_name}" %>
    <outputWriter class="<%= writer['class'] %>">
      <% writer.each do |k, v| %>
        <<%= k %>><%= v %></<%= k %>>
      <% end %>
    </outputWriter>
  <% end  %>

  <collectIntervalInSeconds><%= @collect_interval_in_seconds %></collectIntervalInSeconds>
</jmxtrans-agent>
